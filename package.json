{
  "name": "@theresamclaird/atomic",
  "version": "0.0.57",
  "description": "React component library",
  "author": "Theresa McLaird <theresa@mclairdfamily.com>",
  "license": "UNLICENSED",
  "module": "dist/index.js",
  "main": "dist/cjs/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svg.js"
    },
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/.plop-templates"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/src/**/*.stories.jsx",
      "!<rootDir>/src/**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext js,jsx src/",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "plop": "plop",
    "patch": "npm version patch --force && npm run build && npm publish",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,html,css}": "prettier --write",
    "src/**/*.{js,jsx}": "eslint"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.1.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "nanoid": "^4.0.2",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@storybook/addon-a11y": "^7.0.22",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/addon-mdx-gfm": "^7.0.22",
    "@storybook/manager-api": "^7.0.21",
    "@storybook/react": "^7.0.21",
    "@storybook/react-webpack5": "^7.0.21",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.21",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-loader": "^8.2.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.41.4",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-visualizer": "^4.2.0",
    "storybook": "^7.0.21"
  }
}
